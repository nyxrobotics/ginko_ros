<?xml version="1.0"?>


<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:macro name="body_xacro" params="model_mode">
		<!-- chest -->
		<link name="body_link0">
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/ginko_rs406/stl/link00_chest.stl"/>
				</geometry>
			</collision>
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://ginko_description/meshes/ginko_rs406/obj/link00_chest.obj"/>
				</geometry>
			</visual>
			<inertial>
				<mass value="1.41502697"/>
				<origin xyz="0.00008301 0.00000908 0.03819562" rpy="0 0 0"/>
				<inertia
					ixx="0.00248548"
					ixy="-0.00000074" iyy="0.00121388"
					ixz="0.000003761" iyz="-0.00000042" izz="0.00287756"/>
			</inertial>
		</link>
		<!-- Joint1 -->
		<joint name="body_joint1" type="revolute">
			<parent link="body_link0"/>
			<child  link="body_link1"/>
			<origin xyz="0.0 0 0" rpy="0 0 0"/>
			<axis xyz="0 0 1"/>
			<limit effort="1000.0" lower="-2.6" upper="2.6" velocity="10.0"/>
			<dynamics damping="0.1" friction="0.1"/>
		</joint>
		<!-- waist -->
		<link name="body_link1">
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/ginko_rs406/stl/link01_waist.stl"/>
	    		</geometry>
			</collision>
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://ginko_description/meshes/ginko_rs406/obj/link01_waist.obj"/>
				</geometry>
			</visual>
			<inertial>
				<mass value="0.33312665"/>
				<origin xyz="-0.01135452 -0.00000134 -0.02720222" rpy="0 0 0"/>
				<inertia
					ixx="0.00015328"
					ixy="0.00000003" iyy="0.00073745"
					ixz="0.00001078" iyz="0.00000000" izz="0.00073107"/>
			</inertial>
		</link>
	</xacro:macro>
</robot>
